    "def sieve(n):\n",
    "    if n < 2:\n",
    "        return []\n",
    "    s = [0, 0] + [1] * (n - 1)\n",
    "    for i in range(2, int(n**.5)+1):\n",
    "        if s[i]:\n",
    "            s[i*2::i] = [0] * ((n - i)//i)\n",
    "    return [i for i, v in enumerate(s) if v]\n",
    "\n",
    "print(len(sieve(1000)))"
   ]
